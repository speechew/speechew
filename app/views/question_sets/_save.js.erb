$("ul.errors").html("")
<% if @question_set.errors.any? %>
  <% @question_set.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>  
  $('#question-sets-table').DataTable().ajax.reload();
  $("#global-modal").modal("hide")
<% end %>