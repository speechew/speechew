<%= form_for(@feedback, remote: true, html: {class: :feedback_form, class: "form-horizontal", style: "display:inline;" }) do |f| %>
  <div class="modal-body">
  <ul class="errors"></ul>

  <div class="form-group col-md-6">
    <%= f.label :given_to_id, class: "control-label" %>
	  <%= f.number_field :given_to_id, class: 'form-control' %>
	</div>
  </div>
  <div class="form-group col-md-6">
    <%= f.label :given_by_id, class: "control-label" %>
	  <%= f.number_field :given_by_id, class: 'form-control' %>
	</div>
  </div>
  <div class="form-group col-md-6">
    <%= f.label :fluency, class: "control-label" %>
	  <%= f.number_field :fluency, class: 'form-control' %>
	</div>
  </div>
  <div class="form-group col-md-6">
    <%= f.label :vocabulary, class: "control-label" %>
	  <%= f.number_field :vocabulary, class: 'form-control' %>
	</div>
  </div>
  <div class="form-group col-md-6">
    <%= f.label :grammar, class: "control-label" %>
	  <%= f.number_field :grammar, class: 'form-control' %>
	</div>
  </div>
  <div class="form-group col-md-6">
    <%= f.label :notes, class: "control-label" %>
	  <%= f.text_area :notes, class: 'form-control' %>
	</div>
  </div>
  <div class="modal-footer">
    <%= f.submit "Save Feedback", class: "btn btn-primary" %>
    <%= link_to "Cancel", "#", class: "btn btn-secondary", data: {dismiss: "modal"} %>
  </div>
  </div>
<% end %>
